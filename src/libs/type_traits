#ifndef _YOUMIX_TYPE_TRAITS_HPP_
#define _YOUMIX_TYPE_TRAITS_HPP_

namespace std {
template<typename...> struct function_type;

template<typename R>
struct function_type<R()> {
    using type = R();
};

template<typename R>
struct function_type<R> {
    using type = R();
};

template<typename R, typename... Args>
struct function_type<R(Args...)> {
    using type = R(Args...);
};

}
#endif
